{
    "name": "React Fetch Tree",
    "version": "0.1",
    "manifest_version": 1,
    "description": "React data request tool in the Chrome developer console.",
    "devtools_page": "devtools.html",
    "browser_action": {
      "default_popup": "./popup.html",
      "default_title": "React Fetch Tree"
    },
    "background": {
      "scripts": ["background.js"],
      "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["contentScript.js"]
      }
    ],
    "permissions": ["storage", "activeTab", "<all_urls>", "tabs"]
  }
  